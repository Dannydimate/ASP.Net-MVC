@using WebApplication8.Models;
@model List<Supermercado>


@{
    ViewData["title"] = "Index";
}

@if (Model.Count != 0)
{
    <table class="table table-info" method="post" asp-action="Modificar" asp-controller="Supermercado">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">NameSuper</th>
                <th scope="col">Direccion</th>
                <th scope="col">Autoservicio</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var super in Model)
            {
                <tr>
                    <td>@super.Id</td>
                    <td>@super.NameSuper</td>
                    <td>@super.Direccion</td>
                    <td>@super.Autoservicio</td>
                </tr>
            }
        </tbody>

    </table>

    <div class="form-group">
        <div class="row">
            <div class="col-sm-2 offset-sm-2 ">
                <input type="submit" value="Crear" class="btn btn-primary form-control" />
            </div>
            <div class="col-sm-4">
                <button type="submit" class="btn btn-primary" onclick="Index">Editar</button>
            </div>
        </div>
    </div>

}
else
{
    <div class="card">
        <div class="card-body">
            No hay datos.
        </div>
    </div>
}