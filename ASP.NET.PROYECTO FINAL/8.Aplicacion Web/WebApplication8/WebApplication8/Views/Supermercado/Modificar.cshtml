@using WebApplication8.Models;
@model List<Supermercado>

@{
    ViewData["Title"] = "Modificar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count != 0)
{
    <table class="table table-info">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">NameSuper</th>
                <th scope="col">Direccion</th>
                <th scope="col">Autoservicio</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var super in Model)
            {
                <tr>
                    <td>@super.Id</td>
                    <td>@super.NameSuper</td>
                    <td>@super.Direccion</td>
                    <td>@super.Autoservicio</td>
                </tr>
            }
        </tbody>

    </table>
}
else
{
    <div class="card">
        <div class="card-body">
            No hay datos.
        </div>
    </div>
}